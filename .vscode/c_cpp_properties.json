{
    "configurations": [
        {
            "name": "OLC",
            "includePath": [
                "olcPixelGameEngine/include/**"
            ],
            "defines": [],
            "macFrameworkPath": [
                "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX10.14.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "${default}",
            "compilerArgs": [
                "-g",
                "-arch",
                "x86_64",
                "-std=c++17",
                "-Wall",
                "-framework",
                "Cocoa",
                "-framework",
                "Quartz",
                "-framework",
                "OpenGL",
                "-lcocoawindowing_s",
                "-lpng",
                "-L",
                "olcPixelGameEngine/bin"
            ]
        }
    ],
    "version": 4
}